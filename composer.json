{
    "name": "tenantcloud/laravel-cors",
    "description": "Send CORS headers in a Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
		{
			"name": "TenantCloud",
			"role": "Developer"
		}
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TenantCloud\\Cors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TenantCloud\\Cors\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TenantCloud\\Cors\\CorsServiceProvider"
            ]
        }
    }
}
